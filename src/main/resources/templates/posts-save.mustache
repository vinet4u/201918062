{{>layout/header}}
<h1> 게시글 등록 </h1>

<div class="col-md-12">
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label for="id">ID</label>
                <input type="text" class="form-control" id="id" placeholder="제목을 입력하세요">
            </div>
           <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="name" placeholder="제목을 입력하세요">
            </div>
            <div class="form-group">
                <label for="tel">Phone Number</label>
                <input type="text" class="form-control" id="tel" placeholder="작성자를 입력하세요">
            </div>
            <div class="form-group">
                <label for="address">Address</label>
                <textarea class="form-control" id="address" placeholder="내용을 입력하세요"></textarea>
            </div>
            <div class="form-group">
                <label for="content">Introduction</label>
                <textarea class="form-control" id="intro" placeholder="내용을 입력하세요"></textarea>
            </div>
        </form>

        <a href="/" role="button" class="btn btn-secondary">취소</a>
        <button type="button" class="btn btn-primary" id="btn-save">등록</button>

    </div>
</div>
{{>layout/footer}}